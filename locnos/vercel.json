{
    "version": 2,
    "builds": [
        {
            "src": "frontend-nextjs/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "frontend-nextjs/$1"
        }
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "@api_url"
    }
}